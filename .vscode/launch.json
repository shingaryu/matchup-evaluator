  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\calc-matchup-evaluations.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Damage array calc",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\calcDamageArray.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Damage array calc (typescript)",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeArgs": [
        "-r",
        "ts-node/register"
      ],
      "program": "${workspaceFolder}\\calcDamageArraySplash.ts"
    }
  ]
}